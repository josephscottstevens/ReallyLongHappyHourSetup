<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Blueprints>
    <!-- ORE TO INGOTS -->
    <Blueprint>
      <Id>
        <TypeId>BlueprintDefinition</TypeId>
        <SubtypeId>OsmiumOreToIngot</SubtypeId>
      </Id>
      <DisplayName>OsmiumIngot</DisplayName>
      <Icon>Textures\GUI\Icons\ingot\silver_ingot.dds</Icon>
      <Prerequisites>
        <Item Amount="1" TypeId="Ore" SubtypeId="OsmiumOre" />
      </Prerequisites>
      <Result Amount="0.005" TypeId="Ingot" SubtypeId="Osmium" />
      <BaseProductionTimeInSeconds>4</BaseProductionTimeInSeconds>
    </Blueprint>

    <!-- INGOTS TO COMPONENTS -->
    <Blueprint>
      <Id>
        <TypeId>BlueprintDefinition</TypeId>
        <SubtypeId>Nanotubes</SubtypeId>
      </Id>
      <DisplayName>Nanotubes</DisplayName>
      <Icon>Textures\nanotubes.dds</Icon>

      <Prerequisites>
        <Item Amount="14" TypeId="Ingot" SubtypeId="StrangeMatterCanister" />
        <Item Amount="250" TypeId="Ingot" SubtypeId="Iron" />
        <Item Amount="210" TypeId="Ingot" SubtypeId="Silicon" />
        <Item Amount="120" TypeId="Ingot" SubtypeId="Platinum" />
      </Prerequisites>

      <Result Amount="1" TypeId="Component" SubtypeId="Nanotubes" />
      <BaseProductionTimeInSeconds>100</BaseProductionTimeInSeconds>
      <IsPrimary>true</IsPrimary>
    </Blueprint>
    <Blueprint>
      <Id>
        <TypeId>BlueprintDefinition</TypeId>
        <SubtypeId>MolecularMotor</SubtypeId>
      </Id>
      <DisplayName>Molecular Motor</DisplayName>
      <Icon>Textures\molecular_motor.dds</Icon>

      <Prerequisites>
        <Item Amount="12" TypeId="Ingot" SubtypeId="StrangeMatterCanister" />
        <Item Amount="420" TypeId="Ingot" SubtypeId="Cobalt" />
        <Item Amount="680" TypeId="Ingot" SubtypeId="Nickel" />
        <Item Amount="160" TypeId="Ingot" SubtypeId="Gold" />
        <Item Amount="140" TypeId="Ingot" SubtypeId="Platinum" />
      </Prerequisites>
      
      <Result Amount="1" TypeId="Component" SubtypeId="MolecularMotor" />
      <BaseProductionTimeInSeconds>300</BaseProductionTimeInSeconds>
      <IsPrimary>true</IsPrimary>
    </Blueprint>
    <Blueprint>
      <Id>
        <TypeId>BlueprintDefinition</TypeId>
        <SubtypeId>AdvancedPlate</SubtypeId>
      </Id>
      <DisplayName>Advanced Plate</DisplayName>
      <Icon>Textures\advancedPlating.dds</Icon>
      <Prerequisites>
        <Item Amount="12" TypeId="Ingot" SubtypeId="Osmium" />
        <Item Amount="5" TypeId="Ingot" SubtypeId="Platinum" />
        <Item Amount="3" TypeId="Ingot" SubtypeId="Cobalt" />
      </Prerequisites>
      <Result Amount="1" TypeId="Component" SubtypeId="AdvancedPlate" />
      <BaseProductionTimeInSeconds>2</BaseProductionTimeInSeconds>
      <IsPrimary>true</IsPrimary>
    </Blueprint>

  </Blueprints>
